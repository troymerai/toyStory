#include <stdio.h>

int main(void)
{
	while (1)
	{
		int n = 0;
		int i = 1;
		printf("Enter an integer >> ");
		scanf_s("%d", &n);

		if (n == 0)
		{
			break;
		}

		printf("  Divisors of %d: ", n);


		for (i; i <= n; i++)
		{
			if (n % i == 0)
			{
				printf("%d ", i);
			}
		}

		printf("\n\n");
	}

	printf(" ");

	return 0;
}

#include <stdio.h>

int main()
{
	int arr[9] = {1,3,4,2,7,8,10,12,11};
	
	int sum = 0;
	for (int i = 0; i < 9; i++)
	{
		
		sum = sum + arr[i];
	}
	printf("%d ", sum);

	int max = 0;
	int min = 1;
	for (int i = 0; i < 9; i++)
	{
		if (arr[i] >= max)
		{
			max = arr[i];
		}
		if (arr[i] <= min)
		{
			min = arr[i];
		}
	}
	printf("%d ", max);
	printf("%d ", min);
	
}
